            <!-- OPTIONS --
            <div ng-controller="OptionsController" ng-show="selectedSes.type == 'S'">
                <h4>{{'optionsConfig' | lang}}</h4>
                <div class="row sliders" ng-class="{'bg-gray': selectedSes.status!=1}">
